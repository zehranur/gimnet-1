@model string

@{
    ViewBag.Title = "Yetkisiz Erişim";
    ViewBag.PageId = "NotFound";
    Layout = "~/Views/Shared/_LayoutNoNavigation.cshtml";
}

<div class="page-content">
    <div class="message">
        <a href="javascript:;" onclick="supportClickInit()" class="btn-Support-Destek">
            <i class="fa icon-support"></i>
            <span>Destek</span>
        </a>
        <h1>Hata 404: Sayfa Bulunamadı</h1>
        <h3>Aradığınız kaynak bulunamadı!</h3>
        <a class="redirect" href="~/">Ana sayfa için tıklayınız...</a>
        <div class="date-time">
            @DateTime.Now.ToString("dd/MM/yyyy HH:mm:ss")
        </div>
        <div class="clear"></div>
    </div>
</div>

@{Html.RenderPartial("~/Views/Shared/SupportScript.cshtml");}